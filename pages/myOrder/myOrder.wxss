page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.myOrder-top{
  width:100%;
  height:100rpx;
  background-color: #fff;
  margin-top: 5rpx;
  border-bottom: 1px solid #d2d2d2;
  margin-bottom: 15rpx;
}
.myOrder-top view{
  width:25%;
  height:100rpx;
  float:left;
}
.myOrder-top view text{
  width:80%;
  height:100%;
  line-height:100rpx;
  display: block;
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx;
}
.myOrder-top-text{
  color:#333;
}
.myOrder-top-textClick{
  color:#2abbb4;
  border-bottom:2px solid #2abbb4; 
}
.myOrder-detail{
  width:100%;
}
.myOrder-detail1{
  width:100%;
  background-color: #fff;
  margin-bottom: 10rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.myOrder-detail1-top{
  width:100%;
  height:64rpx;
  border-bottom: 1px solid #ccc;
}
.myOrder-detail1-top text{
  font-size: 28rpx;
  color:#4dc2bc;
  line-height:64rpx;
}
.myOrder-detail1-top text:nth-child(4){
  float:right;
}
.myOrder-detail1-content{
  width:100%;
  height:200rpx;
  border-bottom:1px solid #ccc;
  display: flex;
  flex-direction: row;
  padding-left: 38rpx;
  box-sizing: border-box;
}
.myOrder-detail1-content-img{
  width:140rpx;
  height:140rpx;
  margin-top: 30rpx;
}
.myOrder-detail1-content-view{
  flex: 1;
  font-size: 28rpx;
  padding-left: 10rpx;
  padding-top: 25rpx;
  box-sizing: border-box;
}
.myOrder-detail1-content-view view{
  width: 100%;
  margin-bottom: 5rpx;
}
.myOrder-detail1-content-view view:nth-child(3){
  margin-top: 20rpx;
}
.myOrder-detail1-content-view3-left{
  color:#f00;
}
.myOrder-detail1-content-view3-right{
  float:right;
  color:#4dc2bc;
}
.myOrder-detail1-combined{
  width:100%;
  height:70rpx;
}
.myOrder-detail1-combined view{
  display: inline-block;
  font-size: 28rpx;
  line-height:70rpx;
}
.myOrder-detail1-combined-left text:nth-child(3){
  margin-right: 10rpx;
}
.myOrder-detail1-combined-left text:nth-child(5),.myOrder-detail1-combined-left text:nth-child(6){
  color:#f00;
}
.myOrder-detail1-combined-right{
  float:right;
}
.myOrder-detail1-combined-right text:nth-child(2),.myOrder-detail1-combined-right text:nth-child(3){
  color:#f00;
}
.myOrder-detail1-button{
  width:100%;
  height:80rpx;
}
.myOrder-detail1-button view{
  width:135rpx;
  height:50rpx;
  float:right;
  border: 1px solid #2abbb4;
  color:#2abbb4;
  background-color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height:50rpx;
  border-radius: 20rpx;
  margin-top: 10rpx;
  margin-left: 20rpx;
}
.myOrder-detail2{
  width:100%;
  height:220rpx;
  background-color: #fff;
  margin-bottom:10rpx; 
  padding: 0 30rpx;
  box-sizing: border-box;
}
.myOrder-detail2-content{
  width:100%;
}
.myOrder-detail2-content1,.myOrder-detail2-content2{
  width:100%;
  height:80rpx;
}
.myOrder-detail2-content1 view,.myOrder-detail2-content1 image{
  display: inline-block;
}
.myOrder-detail2-content1 view{
  width:112rpx;
  height:44rpx;
  background-color: #2abbb4;
  color: #fff;
  border-radius: 10rpx;
  font-size: 28rpx;
  text-align: center;
  line-height:44rpx;
  margin-top: 18rpx;
}
.myOrder-detail2-content1 image{
  width:22rpx;
  height:22rpx;
  float: right;
  margin-top: 29rpx;
}
.myOrder-detail2-content2{
  width:100%;
  height:80rpx;
  font-size: 28rpx;
  color:#999;
  line-height:80rpx;
}
.myOrder-detail2-content2 text{
  margin-right: 10rpx;
}
.myOrder-detail3{
  width:100%;
  background-color: #fff;
  margin-bottom: 10rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.myOrder-detail1-button-finish{
  background-color: #fff !important;
  color:#999 !important;
  border:1px solid #999 !important;
}