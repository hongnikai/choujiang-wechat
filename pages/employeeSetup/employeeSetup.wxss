page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.employee-setup{
  width:100%;
}
.employee-setup-all{
  width:100%;
  background-color: #fff;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.employee-setup-I{
  width:100%;
  height:100rpx;
  margin-top: 20rpx;
  border-bottom: 1px solid #cccccc;
}
.employee-setup-I text{
  line-height: 100rpx;
  font-size: 30rpx;
}
.employee-setup-I image{
  width:22rpx;
  height:22rpx;
  float:right;
  margin-top: 39rpx;
}