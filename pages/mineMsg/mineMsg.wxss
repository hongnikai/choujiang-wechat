page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.mineMsg{
  width:100%;
}
.mineMsg-all{
  width:100%;
  margin-top: 20rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}
.mineMsg-list{
  width:100%;
  border-bottom:1px solid #e5e5e5;
  padding: 20rpx 0;
  box-sizing: border-box;
}
.mineMsg-list-top{
  width:100%;
  height:50rpx;
  margin-bottom: 10rpx;
}
.mineMsg-list-top view{
  display: inline-block;
  font-size: 28rpx;
  line-height:50rpx;
}
.mineMsg-list-top view:nth-child(2){
  float:right;
  color:#999;
}
.mineMsg-list-text{
  width:100%;
  font-size: 28rpx;
  color:#999;
}