.techniqueSchedule{
  width:100%;
  border-top: 20rpx solid #f2f2f2;
}
.techniqueSchedule-photo1{
  width:100%;
  height:380rpx;
  border-bottom:1rpx solid #ccc;
  padding:34rpx;
  box-sizing: border-box;
}
.techniqueSchedule-photo1-top{
  width:100%;
  height:70rpx;
  border-bottom:1rpx solid #e5e5e5;
  font-size: 28rpx;
  line-height:70rpx;
}
.techniqueSchedule-photo1-img{
  width:100%;
  height:230rpx;
  padding-top:20rpx;
}
.techniqueSchedule-photo1-img image{
  width:33%;
  height:210rpx;
  display: inline-block;
}
.techniqueSchedule-photo1-img image:nth-child(2){
  margin: 0 0.5%;
}
.techniqueSchedule-sure{
  width:92%;
  height:100rpx;
  background-color: #2abbb4;
  color:#fff;
  font-size: 35rpx;
  text-align: center;
  line-height:100rpx;
  margin: 0 auto;
  margin-top: 80rpx;
  margin-bottom: 60rpx;
  border-radius: 40rpx; 
}
.techniqueSchedule-photo1-text{
  width:100%;
  height:230rpx;
  padding-top:20rpx;
}
.techniqueSchedule-photo1-text textarea{
  width:100%;
  height:100%;
  border: 1px solid #ccc;
  text-indent: 15rpx;
  padding: 10rpx 0;
  box-sizing: border-box;
  resize: none;
  outline: none;
  font-size: 28rpx;
}