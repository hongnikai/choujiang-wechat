page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.employeesList{
  width:100%;
  margin-top: 20rpx;
  padding:0 30rpx 0 30rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.employeesList-list{
  width:100%;
  height:100rpx;
  border-bottom:1rpx solid #ccc;
}
.employeesList-list text{
  line-height:100rpx;
  font-size: 30rpx;
}
.employeesList-list1{
  float:right;
  color:#2abbb4;
}
.employeesList-list2{
  float:right;
  color:#f00;
}