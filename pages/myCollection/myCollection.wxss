page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.myCollection{
  width:100%;
}
.myCollection-all{
  width:100%;
  margin-top: 20rpx;
}
.myCollection-text{
  width:50%;
  height:480rpx;
  float:left;
  background-color: #fff;
}
.myCollection-text image{
  width:100%;
  height:375rpx;
}
.myCollection-text1{
  width:100%;
  font-size: 28rpx;
  color:#000;
  text-align: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.myCollection-text2{
  width:100%;
  font-size: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  color:#e60012;
  text-align: center;
}