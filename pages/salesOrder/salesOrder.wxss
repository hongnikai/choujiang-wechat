page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.salesOrder{
  width:100%;
  margin-top: 20rpx;
}
.salesOrder-detail{
  width:100%;
  background-color: #fff;
  margin-top: 5rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.salesOrder-detail-top{
  width:100%;
  height:60rpx;
  border-bottom: 1rpx solid #ccc;
}
.salesOrder-detail-top text{
  font-size: 28rpx;
  color:#333;
  line-height:60rpx;
}
.salesOrder-detail-top text:nth-child(4){
  float: right;
  color:#2abbb4;
}
.salesOrder-detail1-content{
  width:100%;
  height:200rpx;
  display: flex;
  flex-direction: row;
  padding-left: 38rpx;
  box-sizing: border-box;
}
.salesOrder-detail1-content-img{
  width:140rpx;
  height:140rpx;
  margin-top: 30rpx;
}
.salesOrder-detail1-content-view{
  flex: 1;
  font-size: 28rpx;
  padding-left: 10rpx;
  padding-top: 25rpx;
  box-sizing: border-box;
}
.salesOrder-detail1-content-view view{
  width: 100%;
  margin-bottom: 5rpx;
}
.salesOrder-detail1-content-view view:nth-child(3){
  margin-top: 20rpx;
}
.salesOrder-detail1-content-view3-left{
  color:#f00;
}
.salesOrder-detail1-content-view3-right{
  float:right;
  color:#4dc2bc;
}
.salesOrder-note{
  width:100%;
  height:200rpx;
  background-color: #fff;
  border-bottom: 1px solid #e8e8e8;
  margin-bottom: 10rpx;
  padding-top:36rpx;
  box-sizing: border-box;
}
.salesOrder-note text{
  color:#999;
  font-size:28rpx;
}