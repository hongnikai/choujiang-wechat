page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.installerOrder{
  width:100%;
  margin-top:20rpx;
}
.installerOrder-order{
  width:100%;
  padding: 10rpx 30rpx 10rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-bottom:1rpx solid #e5e5e5;
}
.installerOrder-order-top{
  width:100%;
  height:60rpx;
  border-bottom:1px solid #ccc;
}
.installerOrder-order-top view{
  display: inline-block;
  height:60rpx;
  font-size: 28rpx;
  line-height:60rpx;
  color:#2abbb4;
}
.installerOrder-order-top view:nth-child(2){
  float:right;
}
.installerOrder-order-mid{
  width:100%;
  height:75rpx;
  margin-top:20rpx;
}
.installerOrder-order-mid view,image{
  display: inline-block;
}
.installerOrder-order-mid view{
  width:110rpx;
  height:45rpx;
  color:#fff;
  background-color: #2abbb4;
  text-align: center;
  line-height:45rpx;
  margin-top: 15rpx;
  border-radius: 15rpx;
  font-size: 28rpx;
}
.installerOrder-order-mid image{
  width:22rpx;
  height:22rpx;
  float:right;
  margin-top: 26.5rpx;
}
.installerOrder-order-bottom{
  width:100%;
  height:60rpx;
  padding-bottom:20rpx;
  box-sizing: border-box;
}
.installerOrder-order-bottom text{
  font-size: 28rpx;
  line-height:60rpx;
  color:#999;
  margin-right: 10rpx;
}
.installerOrder-order-bottom text:nth-child(5){
 float: right;
}