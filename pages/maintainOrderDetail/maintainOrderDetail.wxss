page {
  -webkit-user-select:none;
  user-select:none;
  width:100%;
  overflow-x:hidden;
  height:100%;
  background-color:#f2f2f2;
}
.maintainOrderDetail{
  width:100%;
}
.maintainOrderDetail-progress{
  width:100%;
  height:82rpx;
  background-color: #2abbb4;
  color: #fff;
  line-height:82rpx;
  text-align: center;
  font-size: 28rpx;
}
.maintainOrderDetail-all{
  width:100%;
  background-color: #fff;
  padding:0 30rpx;
  box-sizing: border-box;
}
.maintainOrderDetail-time{
  width:100%;
  height:60rpx;
  border-bottom: 1px solid #ccc;
}
.maintainOrderDetail-time image{
  width:26rpx;
  height:26rpx;
  vertical-align: middle;
}
.maintainOrderDetail-time text{
  font-size: 28rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.maintainOrderDetail-time-text1{
  float:right;
  line-height:60rpx;
  color:#999;
}
.maintainOrderDetail-local{
  width:100%;
  height:200rpx;
  padding-top:40rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}
.maintainOrderDetail-local-img{
  width:30rpx;
  height:30rpx;
  vertical-align: middle;
  margin-top: 16rpx;
}
.maintainOrderDetail-local-view{
  flex: 1;
  padding-left: 10rpx;
  box-sizing: border-box;
}
.maintainOrderDetail-local-view view{
  width:100%;
}
.maintainOrderDetail-local-view-name{
  width:100%;
  height:50rpx;
}
.maintainOrderDetail-local-view-name text{
  font-size: 28rpx;
  line-height:50rpx;
}
.maintainOrderDetail-local-view-name text:nth-child(3){
  float: right;
}
.maintainOrderDetail-local-view-local text{
  font-size: 28rpx;
}
.maintainOrderDetail-order{
  width:100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-top:2px solid #f2f2f2;
  border-bottom:10rpx solid #f2f2f2;
}
.maintainOrderDetail-order-top{
  width:100%;
  height:60rpx;
  border-bottom:1px solid #ccc;
}
.maintainOrderDetail-order-top view{
  display: inline-block;
  height:60rpx;
  font-size: 28rpx;
  line-height:60rpx;
  color:#2abbb4;
}
.maintainOrderDetail-order-top view:nth-child(2){
  float:right;
}
.maintainOrderDetail-order-mid{
  width:100%;
  height:75rpx;
}
.maintainOrderDetail-order-mid view,image{
  display: inline-block;
}
.maintainOrderDetail-order-mid view{
  width:110rpx;
  height:45rpx;
  color:#fff;
  background-color: #2abbb4;
  text-align: center;
  line-height:45rpx;
  margin-top: 15rpx;
  border-radius: 15rpx;
  font-size: 28rpx;
}
.maintainOrderDetail-order-mid image{
  width:22rpx;
  height:22rpx;
  float:right;
  margin-top: 26.5rpx;
}
.maintainOrderDetail-order-bottom{
  width:100%;
  height:60rpx;
  padding-bottom:20rpx;
  box-sizing: border-box;
}
.maintainOrderDetail-order-bottom text{
  font-size: 28rpx;
  line-height:60rpx;
  color:#999;
  margin-right: 10rpx;
}
.maintainOrderDetail-order-bottom text:nth-child(5){
 float: right;
}
.maintainOrderDetail-answer{
  width:100%;
  background-color: #fff;
  margin-top: 20rpx;
  padding: 20rpx;
  box-sizing: border-box;
}
.maintainOrderDetail-answer view{
  width:100%;
  font-size: 28rpx;
  margin-bottom: 15rpx;
}